{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Stmt1513517192392",
      "Action": [
        "dynamodb:Query",
        "dynamodb:GetRecords",
        "dynamodb:GetItem",
        "dynamodb:Scan",
